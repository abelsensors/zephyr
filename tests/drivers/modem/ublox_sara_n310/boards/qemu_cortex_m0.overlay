/* SRAM and FLASH sizes are increased to match that of the stm32l152c */
&sram0 {
	reg = <0x20000000 DT_SIZE_K(32)>;
};

&flash0 {
	reg = <0x08000000 DT_SIZE_K(256)>;
};

&uart0 {
    status = "okay";
    current-speed = <57600>;

    saran310 {
        compatible = "ublox,sara-n310";
        label = "SARAN310";
    };
};
